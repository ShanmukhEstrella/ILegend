@top Program { Statement* }

Statement {
  Keyword | Number | Operator | LineComment | Identifier | Parens
}

@skip { whitespace }

@tokens {
  whitespace { "[ \\t\\r\\n]+" }
  LineComment { "#[^\\n]*" }
  Keyword { "load" | "db" | "drop_all_tables" }
  Number { "[0-9]+" }
  Operator { "+" | "-" | "*" | "/" | "=" | "<" | ">" | "!" }
  Identifier { "[a-zA-Z][a-zA-Z0-9]*" }
  Parens { "(" | ")" }
}
